.swatch{
  	margin:0px 0px 10px;
  	@include clearfix;
	.header {
        margin:0px;
        padding:10px 0px;
        float:left;
        width:100%;
      	color:$vela-text-color-primary;
    }
  	input { 
        display:none;
    }
  	label {
        border-radius:2px;
        float:left;
        min-width:35px !important; 
        height:35px !important;
        margin:0;
        background-color:white;
        border:$vela-border-main;
        color: $vela-text-color-primary;
        font-size:$vela-font-size;
        font-weight:normal;
        text-align:center;
        line-height:35px;
        white-space:nowrap;
        text-transform:uppercase;
    }
  	.swatch-element {
        float:left;
        @include transform(translateZ(0));
        -webkit-font-smoothing:antialiased;
        margin:0px 10px 0px 0;
        position:relative;
      	&.soldout{
        	display: none;
        }
    }
}
.swatch-element label { padding:0 10px; cursor: pointer;}
.color.swatch-element label { padding:0; }
.swatch input:checked + label {
    @include box-shadow(0 2px 5px 0 rgba(0,0,0,0.16),0 2px 10px 0 rgba(0,0,0,0.12));
    border-color:transparent;
} 
.crossed-out { position:absolute; width:100%; height:100%; left:0; top:0; }
.swatch .swatch-element .crossed-out { display:none; }
.swatch .swatch-element.soldout .crossed-out { display:block; }
.swatch .swatch-element.soldout label {
    @include opacity(0.6);
}
.swatch .tooltip {
    text-align:center;
    background:gray;
    color:#fff;
    bottom:100%;
    padding: 10px;
    display:block;
    position:absolute;
    width:100px;
    left:-30px;
    margin-bottom:15px;
    visibility:hidden;
    @include opacity(0);
  	@include transform(translateY(10px));
    @include transition(all .25s ease-out);
    @include box-shadow(2px 2px 6px rgba(0, 0, 0, 0.28));
    @include box-sizing(border-box);
}
.swatch .tooltip:before {
    bottom:-20px;
    content:"";
    display:block;
    height:20px;
    left:0;
    position:absolute;
    width:100%;
}
.swatch .tooltip:after {
    border-left:solid transparent 10px;
    border-right:solid transparent 10px;
    border-top:solid gray 10px;
    bottom:-10px;
    content:"";
    height:0;
    left:50%;
    margin-left:-13px;
    position:absolute;
    width:0;
}
.swatch .swatch-element:hover .tooltip {
    visibility:visible;
    @include opacity(1);
    @include transform(translateY(0px));
}
.swatch.error {
    background-color:#E8D2D2!important;
    color:#333!important;
    padding:1em;
    border-radius:5px;
}
.swatch.error p {
    margin:0.7em 0;
}
.swatch.error p:first-child {
    margin-top:0;
}
.swatch.error p:last-child {
    margin-bottom:0;
}
.swatch.error code {
    font-family:$font-family-monospace;
}
