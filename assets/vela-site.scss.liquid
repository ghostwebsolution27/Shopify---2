// Vela Panel Config Sesction
.menuSectionConfig{
  	display: none;
  	&.showBox{
    	display: block;
    }
  	.menuSsWrap{
      	position: fixed;
        top: 50px;
        right: 0;
        z-index: 9999;
      	display: block;
    	width: 260px;
      	background-color: #fff;
      	@include translate(260px , 0);
  		@include transition(all 0.35s ease);
  		@include box-shadow(0 4px 5px 0 rgba(0,0,0,0.14),0 1px 10px 0 rgba(0,0,0,0.12),0 2px 4px -1px rgba(0,0,0,0.3));
      	&.active{
        	@include translate(0px , 0);
          	.btnConfigSection{
              	visibility: hidden;
              	z-index: -1;
              	@include opacity(0);
            	@include translate(40px , 0);
            }
        }
    }
  	
}
.menuSsHeading{
	margin: 0;
  	padding: 10px 20px;
  	color: #fff;
  	background-color: #333;
  	font-size: 16px;
  	text-transform: uppercase;
}
.closeMenu{
	position: absolute;
  	top: 10px;
  	right: 15px;
  	width: 20px;
  	height: 20px;
  	cursor: pointer;
  	&:before,&:after{
    	content: "";
      	display: block;
      	position: absolute;
      	top: 8px;
      	left: 2px;
      	width: 16px;
      	height: 2px;
      	background-color: #fff;
    }
  	&:before{
    	@include rotate(45deg);
    }
  	&:after{
    	@include rotate(-45deg);
    }
}
.btnConfigSection{
  	display: block;
  	position: absolute;
  	top: 0;
  	left: -40px;
  	width: 40px;
  	height: 40px;
  	color: #fff;
  	background-color: $vela-color-primary;
	font-size: 20px;
  	line-height: 40px;
  	text-align: center;
  	cursor: pointer;
  	visibility: visible;
    z-index: 1;
    @include opacity(1);
  	@include box-shadow(0 2px 2px 0 rgba(0,0,0,0.14),0 1px 5px 0 rgba(0,0,0,0.12),0 3px 1px -2px rgba(0,0,0,0.2));
  	@include transition(all 0.35s ease);
}
.menuListPage{
	margin: 0;
  	padding: 15px;
  	>li{
      	margin-bottom: 10px;
      	&:last-child{
        	margin-bottom: 0;
        }
      	a{
        	display: block;
          	padding: 8px 15px;
          	color: #fff;
          	background-color: $vela-color-primary;
          	text-decoration: none;
          	@include transition(all 0.35s ease);
          	@include box-shadow(0 2px 2px 0 rgba(0,0,0,0.14),0 1px 5px 0 rgba(0,0,0,0.12),0 3px 1px -2px rgba(0,0,0,0.2));
          	&:hover{
            	color: #fff;
          		background-color: darken($vela-color-primary, 15%);
          		@include box-shadow(0 4px 5px 0 rgba(0,0,0,0.14),0 1px 10px 0 rgba(0,0,0,0.12),0 2px 4px -1px rgba(0,0,0,0.3));
            }
        }
    }
}

