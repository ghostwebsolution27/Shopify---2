//========== VELA MODULE SASS ==========//
//========= VERSION1.0 - DUCHV =========//
//---------- I. Slick Carousel ----------//
@function slick-image-url($url) {
  	@return url({{ "loading.gif" | asset_url }});
}
@function slick-font-url($url) {}
.slick-slider {
    position: relative;
    display: block;
    box-sizing: border-box;
    -webkit-touch-callout: none;
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    -ms-touch-action: pan-y;
    touch-action: pan-y;
    -webkit-tap-highlight-color: transparent;
    .slick-track,.slick-list{
        @include transform(translate3d(0, 0, 0));
    }
}
.slick-list {
    position: relative;
    overflow: hidden;
    display: block;
    margin: 0;
    padding: 0;
    &:focus {
        outline: none;
    }
    &.dragging {
        cursor: pointer;
        cursor: hand;
    }
}
.slick-track {
    position: relative;
    left: 0;
    top: 0;
    display: block;
    &:before,&:after {
        content: "";
        display: table;
    }
    &:after {
        clear: both;
    }
    .slick-loading & {
        visibility: hidden;
    }
}
.slick-slide {
    float: left;
    height: 100%;
    min-height: 1px;
    display: none;
    [dir="rtl"] & {
        float: right;
    }
    &.slick-loading img {
        display: none;
    }
    &.dragging img {
        pointer-events: none;
    }
    .slick-initialized & {
        display: block;
    }
    .slick-loading & {
        visibility: hidden;
    }
    .slick-vertical & {
        display: block;
        height: auto;
        border: 1px solid transparent;
    }
}
.slick-arrow.slick-hidden {
    display: none;
}
//---------- Slick Slider Theme ----------//
.slick-list {
    .slick-loading & {
        background: #fff slick-image-url("loading.gif") center center no-repeat;
    }
}
.slick-prev,.slick-next {
    position: absolute;
    top:calc(50% - 20px);
    z-index:2;
    display: block;
    color:#fff;
    border:0px;
    font-size:0px;
    height: 36px;
    line-height: 38px;
    text-align: center;
    transition: all 300ms ease-in 0s;
    width: 66px;
}
.slick-prev {
	left:-70px;
	background: url({{'arrow-slide.png' | asset_url }}) left top no-repeat;
	&:hover {
		background: url({{'arrow-slide.png' | asset_url }}) left bottom no-repeat;
	}
	@media(max-width: 992px) {
		left: 0;
	}
}
.slick-next {
	right:-70px;
	background: url({{'arrow-slide.png' | asset_url }}) right top no-repeat;
	&:hover {
		background: url({{'arrow-slide.png' | asset_url }}) right bottom no-repeat;
	}
	@media(max-width: 992px) {
		right: 0;
	}
}
.slick-dotted.slick-slider {
    //margin-bottom: 30px;
}
.slick-dots {
    position: absolute;
    bottom: -15px;
    list-style: none;
    display: block;
    text-align: center;
    padding: 0;
    margin: 0;
    width: 100%;
    li {
        display:inline-block;
      	position:relative;
      	margin:0 5px;
        padding: 0;
        width:20px;
      	height:20px;
        cursor: pointer;
        button {
            display:block;
          	color:$vela-text-color-primary;
          	background-color:#f5f5f5;
          	border:0px;
          	font-size:11px;
        }
        &.slick-active button{
          	color:#fff;
          	background-color:$vela-color-primary;
        }
    }
}
//---------- II. Vela Slideshow ----------//
$slideshow-loader:#FFF;
$z-index-slideshow-image:1;
$z-index-slideshow-video:2;
$z-index-slideshow-text:3;
$z-index-slideshow-controls:4;
$color-slideshow-close-bg:#FFF;
$color-slideshow-close-text:#000;
.velaSlideshowWrapper{
  	position: relative;
}
.video-loader {
    @include spinner(40px, $slideshow-loader);
    @include animation(spin 0.65s infinite linear);
    transition: all 0.1s ease-out 0.5s;
    z-index: $z-index-slideshow-controls;
    top: 50%;
    left: 50%;
    .ie9 &,
    .video-is-loaded &,
    .video-is-playing &,
    .video-is-paused &,
    .autoplay &,
    .no-autoplay & {
      	content: none;
      	display: none;
    }
    .video-is-loading &,
    .autoplay .video-is-loading &,
    .no-autoplay .video-is-loading & {
      	display: block;
      	visibility: visible;
      	opacity: 1;
    }
}
.velaSlideshow{
  	.slideshow{
    	overflow: hidden;
    }
  	.velassSlide,
    .slick-list,
    .slick-track {
      	height: 100%;
    }
  	.slick-prev,
    .slick-next {
      	
    }
    
    .slick-dots {
        bottom:5px;
        text-align:center;
        width: 100%;
        li button::before {
          	color:#FFF;
        }
   }
   &:hover {
   	.slick-prev {
    	  	left: 40px;
    	}
    	.slick-next {
      	right: 40px;
    	}
   }
}
.video-is-playing .slick-dots {
  	display: none !important;
}
.btnssPause:focus {
    clip: auto;
    width: auto;
    height: auto;
    margin: 0;
    color:#FFF;
    background-color:#333;
    padding:$vela-gutter-width / 2;
    z-index:10;
    transition: none;
    .video-is-playing & {
      	display: none;
    }
}
.btnssPauseStop {
    display: block;
    .is-paused & {
      	display: none;
    }
}
.btnssPausePlay {
  	display: none;
  	.is-paused & {
    	display: block;
  	}
}
.velassSlide {
    position: relative;
    overflow: hidden;
}

.velassLink{
  	display: block;
  	&:active,
  	&:focus {
    	opacity: 1;
  	}
}
.velassImage {
    transition:opacity 0.8s cubic-bezier(0.44, 0.13, 0.48, 0.87);
    z-index: $z-index-slideshow-image;
  	.slick-initialized &,
  	.no-js & {
    	opacity: 1;
  	}
    .velassSlideBackgroundVideo & {
      	opacity: 0;
    }
  	.no-autoplay & {
    	opacity: 1;
  	}
}
.ie9 {
  	.velassSlide {
    	z-index: 1 !important;
  	}
  	.slick-dots {
    	z-index: 2;
  	}
}
.velassCaption {
  	.slideshow__link & {
    	cursor: inherit;
  	}
  	.slideshow__slide--has-background-video & {
    	padding-top: $vela-gutter-width * 3;
  	}
  	.video-is-playing & {
    	display: none;
  	}
  	.velassSlide.video-is-paused & {
    	display: none;
  	}
}
.velassCaptionContent {
  	text-align: center;
  	position: absolute;
  	width: 100%;
  	top: 50%;
  	@include transform(translateY(-40%));
  	opacity: 0;
  	transition: all 0.6s cubic-bezier(0.44, 0.13, 0.48, 0.87);
  	transition-delay: 0.3s;
  	z-index: $z-index-slideshow-text;
  	.slick-active &,
  	.no-js & {
    	@include transform(translateY(-50%));
    	opacity: 1;
  	}
    &::after {
        content:"";
        @include spinner(40px, $slideshow-loader);
        @include animation(spin 0.65s infinite linear);
        opacity: 1;
        transition: all 1s cubic-bezier(0.29, 0.63, 0.44, 1);
        bottom: -$vela-gutter-width;
        left: 50%;
    }
    .slick-initialized &,.no-js & {
        &::after {
            opacity: 0;
            visibility: hidden;
            content: none;
        }
    }
}
.velassVideo{
    display: none;
    position: absolute;
    left: 0;
    top: 0;
    z-index: $z-index-slideshow-video;
}
.velassVideoBackground {
  	visibility: hidden;
  	opacity: 0;
  	transition: all 0.2s ease-in;
  	.autoplay &.video-is-loaded {
        display: block;
        visibility: visible;
        opacity: 1;
        position:absolute;
  	}
}
.velassSlideBackgroundVideo::after {
    content: '';
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: $z-index-slideshow-video;
}

.velassVideo-chrome{
  	display: none;
  	opacity: 0;
  	visibility: none;
  	width: 100%;
  	height: 100%;
  	transition: all 0.2s ease-in;
  	// YouTube video will not load if iframe is display: none
  	.ie9 & {
    	display: block;
  	}
    .velassSlide.video-is-playing &,
    .velassSlide.video-is-paused & {
      	display: block;
      	visibility: visible;
      	opacity: 1;
    }
}
.btnssVideoControl{
    display: none;
    visibility: hidden;
    opacity: 0;
    position: absolute;
    padding: 5px;
    z-index: $z-index-slideshow-controls;
    transition: all 0.1s ease-out;
    &:hover,
    &:focus {
      	opacity: 0.7;
    }
}
.velassHasCaption {
  	margin-top:$vela-gutter-width;
}
.btnssVideoControlPlay{
  	opacity: 0;
  	position: relative;
  	margin: 0 auto;
  	.velassVideoBackground{
    	top: 50%;
    	@include transform(translateY(-50%));
  	}
  	.video-is-loaded & {
    	display: block;
    	visibility: visible;
    	opacity: 1;
  	}
  	.video-is-loading &,
  	.video-is-playing &,
  	.velassSlide.video-is-paused & {
    	display: none;
    	visibility: hidden;
    	opacity: 0;
  	}
  	.icon {
    	width: 42px;
    	height: 100%;
    	fill: currentColor;
  	}
}
.btnssVideoControlClose {
  	top: 10px;
  	right: 10px;
  	background-color: $color-slideshow-close-bg;
  	color: $color-slideshow-close-text;
  	.video-is-playing &,
  	.velassSlide.video-is-paused & {
    	display: block;
    	visibility: visible;
    	opacity: 1;
  	}
}
.velassNoImagePlaceholder{
  	.icon {
    	border: 0;
  	}
}
//---------- III. Megamenu ----------//
$velamenu-bgcolor:              {{ settings.velamenu_bgcolor }};
$velamenu-linkcolor:            {{ settings.velamenu_linkcolor }};
$velamenu-linkcolor-hover:      {{ settings.velamenu_linkcolor_hover }};
$velamenu-fontsize:             {{ settings.velamenu_fontsize }};
$velasubmenu-bgcolor:           {{ settings.velamenu_dropdown_bgcolor }};
$velasubmenu-linkcolor:         {{ settings.velamenu_dropdown_linkcolor }};
$velasubmenu-linkcolor-hover:   {{ settings.velamenu_dropdown_linkcolor_hover }};
$velasubmenu-fontsize:          {{ settings.velamenu_dropdown_fontsize }};
#velaMegamenu{
    position:relative;
    background-color: $velamenu-bgcolor;
    .menuContainer{
        position:relative;
        margin:0px;
        padding:0px;
    }
}
.velaMegamenu{
  ul{list-style:none;padding:0px;}
  ul.nav{
      margin:0px;
      padding:0px;
      text-align: right;
      >li {
          padding:0px;
          float: none;
          margin: 0;
          display: inline-block;
          vertical-align: middle;
          > a {
              font-family:$vela-font-family-tertiary;
              font-size: $velamenu-fontsize;
              padding: 10px 20px;
              font-weight: 400;
              line-height: 25px;
              text-transform: uppercase;
              background-color:$nocolor;
              color:$velamenu-linkcolor;
              position: relative;
              @include transition(all 0.35s ease);
              span {
              	position: relative;
              }
          }
          &.hasMenuDropdown{
              >a{
                  &:after{
                      content: "\f107";
                      @include inline-block;
                      font-family: $vela-font-icon;
                      float: right;
                      margin-left: 5px;
                  }
              }
              &.hasMegaMenu{
                  position:static;
              }
              &:hover,&:focus{
                  > .menuDropdown{
                      visibility:visible;
                      top: 100%;
                      @include opacity(1);
                  }
              }
          }
          &:hover,
          &.active {
          	> a, > a:hover, > a:focus {
          		background-color: $nocolor;
              color: $velamenu-linkcolor-hover;
          	}
          }
      }
  }
  .menuDropdown{
    background-color: $velasubmenu-bgcolor;
    background-position: right bottom;
    background-repeat: no-repeat;
    display:block;
    position:absolute;
    text-align: left;
    top: 150%;
    left:0px;
    margin:0px;
    padding:10px 20px;
    min-width:220px;
    z-index:900;
    visibility:hidden;
    @include opacity(0);
    @include transition(all 0.3s ease);
  	@include box-shadow(0px 6px 15px rgba(0, 0, 0, 0.12));
    &.megaMenu{
      right:0px;
      width:100%;
      padding: 20px 30px;
    }
  	.menuTitle{
      font-family:$vela-font-family-secondary;
    	font-weight:700;
    	font-size: $velasubmenu-fontsize;
    	border-bottom: $vela-border-main;
    	margin-bottom:10px;
    	color: $velasubmenu-linkcolor;
    	display:block;
    	border-bottom: 0;
    	> span,a{
        font-size: $velasubmenu-fontsize;
        padding: 5px 0;
        background-color: $nocolor;        
        color: $vela-text-color-secondary;
        display:block;
        font-weight: 600;
        text-transform: uppercase;
        border-bottom: 0;
        font-style: inherit;
      }
    }
    li {
      position:relative;
      border-bottom:1px dashed $vela-border-main;
      a{
        font-size:$velasubmenu-fontsize;
        font-weight:normal;
        padding: 10px 0;
        background-color:$nocolor;
        color: $velasubmenu-linkcolor;
        display:block;
        font-style: italic;
        @include transition(all 0.35s ease);
        &:hover{
            background-color:$nocolor;
            color:$velasubmenu-linkcolor-hover;
            text-decoration:none;
        }
      }
      &:last-child {
      	> a {
      		border-bottom: 0;
      	}
      }
      &.menuTitle{
      	border-bottom: 0;
      }
      &.hasMenuDropdown{
        >a{
          &:after{
            content: "\f105";
            @include inline-block;
            font-family: $vela-font-icon;
            float: right;
            margin-left: 5px;
          }
        }
        &:hover,&:focus{
          > .menuDropdown{
            visibility:visible;
            left: 100%;
            @include opacity(1);
          }
        }
      }
    }
  }
}
.velaMenuProducts{
	.listProduct {
		padding-top: 5px;
	}
  	.blockProMenu{
    	@include clearfix;
      	margin-bottom:10px;
      	.proName{
        	margin:0px 0 5px;
            padding:0px;
            a{
                display:block;
                color: $velasubmenu-linkcolor;
                line-height:22px;
              	max-height: 40px;
              	overflow: hidden;
                &:hover,&:focus{
                    color: $velasubmenu-linkcolor-hover;
                    text-decoration:none;
                }
            }
        }
      	.boxProPrice{
        	margin: 5px 0px;
        	font-weight: 700;
          .proPrice {
          	color: $vela-color-primary;
	        	&.oldPrice {
	        		text-decoration: line-through;
	        		color: #b4b4b4;
	        	}
          }
        }
        .proReviews { 
        	line-height: 18px;
        	.spr-badge-caption {
        		display: none;
        	}
        }
    }
}
// MENU MOBILE
@media (max-width:992px){
  	.menuMobileActive{
		height:100%;
  		overflow:hidden;
	}
}
.menuBtnMobile{
	position:relative;
	padding:10px 0px;
}
.btnMenuMobile{
	margin: 0 15px;
	>span {
		display: block;
    	height: 2px;
    	margin: 0 0 3px;
    	position: relative;
    	transition: all 300ms ease 0s;
    	width: 22px;
    	background-color: #fff;
    	z-index: 91;
	}
	@include transition(all 0.35s ease);
	&:hover {
		cursor: pointer;
		span {
			background-color: $vela-color-primary;
		}
	}
}
.menuMobileContainer{
  	-webkit-transform: translateZ(0);
  	will-change: transform;
  	display:block;
  	width:300px;
    position: fixed;
  	top:0;
    bottom:0;
  	left:-300px;
  	z-index:1000;
    overflow-y: auto;
    overflow-x: hidden;
    -webkit-overflow-scrolling: touch;
    padding: 0 ($vela-gutter-width / 2) ($vela-gutter-width / 2);
    color: $vela-text-color-primary;
	background-color: #F6F6F6;
  	visibility:hidden;
  	@include opacity(0);
    @include transition(all 0.4s cubic-bezier(0.46,0.01,0.32,1));
  	.menuMobileActive &{
      	visibility:visible;
  		@include opacity(1);
      	@include transform(translateX(300px));
    }
	.memoHeader{
		position:relative;
		margin:0px;
		padding: 10px 0px;
		border-bottom:$vela-border-main;
		font-weight:bold;
		font-size:$vela-font-size + 6;
		color: $vela-text-color-secondary;
		.close{
			padding: 4px 10px;
		}
	}
	ul {
		list-style:none;
		padding:0px;
		&.memoDropdown {
			border-left: 1px solid $vela-border-color;
			margin-bottom: 15px;
		}
	}
	.memoNav{
		margin:10px 0px;
		> li {			
			border-bottom: $vela-border-main;
		}
		li{
			position:relative;
			a {
				display:block;
				padding: 10px 0px;
				color: $vela-text-color-secondary;
				font-size: 14px;
				font-weight: 600;
				&:hover {
					color: $vela-color-primary;
				}
			}
			.memoBtnDropdown{
				position:absolute;
				top:0px;
				right:0px;
				z-index:1;
				width:40px;
				height:40px;
				text-align:center;
				line-height:38px;
				cursor:pointer;
			}
		}
		.memoDropdown{
			>li{
				padding-left:15px;
			}
		}
	}
}
.menuMobileOverlay{
    display:block;
    position:fixed;
    top:0px;
    left:0px;
    bottom:0px;
    width:100%;
    height:100%;
    z-index:999;
    background-color:rgba(0,0,0,0.8);
  	pointer-events: none;
    visibility:hidden;
    @include opacity(0);
  	@include transition(all 0.4s cubic-bezier(0.46,0.01,0.32,1));
   .menuMobileActive &{
      visibility:visible;
     	@include opacity(1);
   }
}
